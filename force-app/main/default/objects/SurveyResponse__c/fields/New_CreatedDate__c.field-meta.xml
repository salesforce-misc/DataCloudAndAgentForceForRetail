<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>New_CreatedDate__c</fullName>
    <formula>IF(
    CreatedDate &gt;= (NOW() - 1/24),
    TRUE,
    FALSE
)</formula>
